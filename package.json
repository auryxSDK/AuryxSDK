{
  "name": "@auryx/sdk",
  "version": "0.1.0",
  "description": "High-performance decentralized oracle SDK for Solana",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "test": "jest",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepare": "husky install",
    "prepublishOnly": "npm run build",
    "docs": "typedoc"
  },
  "keywords": [
    "solana",
    "oracle",
    "blockchain",
    "defi",
    "cryptocurrency",
    "auryx",
    "sdk"
  ],
  "author": "Auryx Labs",
  "license": "MIT",
  "dependencies": {
    "@solana/web3.js": "^1.87.0",
    "bs58": "^5.0.0",
    "node-fetch": "^2.6.7",
    "tweetnacl": "^1.0.3"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^18.15.0",
    "@types/node-fetch": "^2.6.2",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.40.0",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typedoc": "^0.24.8",
    "typescript": "^5.0.4"
  },
  "engines": {
    "node": ">=16"
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AuryxSDK/AuryxSDK.git"
  },
  "bugs": {
    "url": "https://github.com/AuryxSDK/AuryxSDK/issues"
  },
  "homepage": "https://auryx.dev",
  "publishConfig": {
    "access": "public"
  }
}
